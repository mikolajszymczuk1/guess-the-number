{
  "name": "guess-the-number",
  "version": "1.0.0",
  "description": "Simple JS game where user must guess computer's random number",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "start": "parcel index.html --port 3000",
    "clean": "rm -r dist/*",
    "build:parcel": "parcel build index.html --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikolajszymczuk1/guess-the-number.git"
  },
  "author": "Miko≈Çaj Szymczuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikolajszymczuk1/guess-the-number/issues"
  },
  "homepage": "https://github.com/mikolajszymczuk1/guess-the-number#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^26.0.1",
    "cssnano": "^4.1.10",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.8"
  }
}
